{
  "name": "teseve",
  "version": "0.2.1",
  "description": "A simple static webserver, in an app.",
  "main": "app.js",
  "homepage": "https://github.com/leny/teseve",
  "contributors": [
    {
      "name": "Pierre-Antoine 'leny' Delnatte",
      "email": "info@flatland.be",
      "url": "http://leny.me"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/leny/teseve.git"
  },
  "bugs": {
    "url": "https://github.com/leny/teseve/issues"
  },
  "licenses": [
    {
      "type": "UNLICENSE",
      "url": "https://github.com/leny/teseve/blob/master/LICENSE"
    }
  ],
  "scripts": {
    "build:osx": "./node_modules/.bin/electron-packager ./ Teseve --platform=darwin --arch=all --version=0.32.0 --icon=icon.icns --out=./releases/mac --ignore=./releases --ignore=./node_modules/electron-builder",
    "build:win32": "./node_modules/.bin/electron-packager ./ Teseve --platform=win32 --arch=ia32 --version=0.32.0 --icon=icon.ico --out=./releases/win32 --ignore=./releases --ignore=./node_modules/electron-builder",
    "build:win64": "./node_modules/.bin/electron-packager ./ Teseve --platform=win32 --arch=x64 --version=0.32.0 --icon=icon.ico --out=./releases/win64 --ignore=./releases --ignore=./node_modules/electron-builder",
    "pack:win32": "npm run build:win32 && ./node_modules/.bin/electron-builder \"./releases/win32/Teseve-win32-ia32\" --platform=win --out=./releases/win32 --config=builder.json",
    "pack:win64": "npm run build:win64 && ./node_modules/.bin/electron-builder \"./releases/win64/Teseve-win32-x64\" --platform=win --out=./releases/win64 --config=builder.json"
  },
  "dependencies": {
    "empty-port": "0.0.1",
    "express": "^4.13.3",
    "express3-handlebars": "^0.5.2",
    "human-size": "^1.1.0",
    "lodash": "^3.10.1",
    "mimetype": "0.0.7",
    "moment": "^2.10.6"
  },
  "devDependencies": {
    "electron-builder": "^2.0.0",
    "electron-packager": "^5.1.0"
  }
}
